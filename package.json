    "gatsby-transformer-yaml": "^2.2.26",
    "giphy-js-sdk-core": "^1.0.6",
    "graphql": "^14.6.0",
    "intersection-observer": "^0.7.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "react-pose": "^4.0.10",
