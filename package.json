    "gatsby-transformer-yaml": "^3.14.0",
    "intersection-observer": "^0.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "remark": "^13.0.0",
    "remark-breaks": "^2.0.2",
    "remark-html": "^13.0.2",
    "remark-parse": "^9.0.0",
